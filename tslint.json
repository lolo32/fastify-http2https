{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {
  },
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "semicolon": [
      true,
      "never"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ]
  },
  "rulesDirectory": []
}
